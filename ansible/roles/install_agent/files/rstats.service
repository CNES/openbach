[Unit]
Description=Startup script for Rstats
Requires=network.target

[Service]
Type=simple
ExecStart=/opt/openbach/virtualenv/bin/python3 /opt/openbach/agent/rstats/rstats.py
ExecReload=/opt/openbach/virtualenv/bin/python3 /opt/openbach/agent/rstats/rstats_reload.py

[Install]
WantedBy=multi-user.target

