    "moment": "^2.22.2",
    "react": "15.4.2",
    "react-chartist": "0.13.0",
    "react-dom": "15.4.2",
    "react-faux-dom": "3.0.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.5",
    "react-select": "1.0.0-rc.3",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.5.2",
    "redux-form": "6.6.3",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "2.2.0",
    "whatwg-fetch": "1.1.1"
  },
