{
  "name": "openbach-frontend",
  "version": "2.0.3",
  "description": "the first frontend for openbach",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --inline --hot --host 0.0.0.0 --history-api-fallback",
    "build": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "clean": "rm -rf node_modules && rm -rf ../../../ansible/roles/install_auditorium/files/frontend/",
    "prod": "npm run clean && npm install && npm run build",
    "lint": "./node_modules/.bin/tslint -c tslint.json --project tsconfig.json --format stylish",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "author": "viveris technologies",
  "license": "MIT",
  "dependencies": {
